{
  "name": "vue-starter-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode mock",
    "build:test": "npm run type-check && vite build --mode test",
    "build:gray": "npm run type-check && vite build --mode gray",
    "build:prod": "npm run type-check && vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "plop": "plop"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "axios": "^1.4.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-request": "^2.0.3",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.18",
    "@rushstack/eslint-patch": "^1.3.1",
    "@swayjs/prettier-config": "^1.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.3.1",
    "@types/nprogress": "^0.2.0",
    "@unocss/preset-uno": "^0.53.1",
    "@unocss/reset": "^0.53.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.42.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.14.1",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "mockjs": "^1.1.0",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.1.3",
    "unocss": "^0.53.1",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-inspect": "^0.7.28",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  }
}